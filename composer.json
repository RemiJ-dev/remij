{
    "name": "stenope/skeleton",
    "description": "Stenope skeleton for content sites",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "^1.11",
        "phpdocumentor/reflection-docblock": "^5.6",
        "stenope/stenope": "0.x-dev",
        "symfony/asset": "7.4.*",
        "symfony/asset-mapper": "7.4.*",
        "symfony/console": "7.4.*",
        "symfony/dotenv": "7.4.*",
        "symfony/expression-language": "7.4.*",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "7.4.*",
        "symfony/intl": "7.4.*",
        "symfony/mime": "7.4.*",
        "symfony/monolog-bundle": "^3.11",
        "symfony/process": "7.4.*",
        "symfony/property-access": "7.4.*",
        "symfony/property-info": "7.4.*",
        "symfony/runtime": "7.4.*",
        "symfony/serializer": "7.4.*",
        "symfony/stimulus-bundle": "^2.32",
        "symfony/string": "7.4.*",
        "symfony/translation": "7.4.*",
        "symfony/twig-bundle": "7.4.*",
        "symfony/ux-turbo": "^2.32",
        "symfony/yaml": "7.4.*",
        "symfonycasts/sass-bundle": "^0.9",
        "twig/twig": "^3.23"
    },
    "require-dev": {
        "ekino/phpstan-banned-code": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.94",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "symfony/browser-kit": "7.4.*",
        "symfony/css-selector": "7.4.*",
        "symfony/debug-bundle": "7.4.*",
        "symfony/maker-bundle": "^1.66",
        "symfony/phpunit-bridge": "7.4.*",
        "symfony/stopwatch": "7.4.*",
        "symfony/var-dumper": "7.4.*",
        "symfony/web-profiler-bundle": "7.4.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "sass:build": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "post-create-project-cmd": [
            "npm install --color=always --no-progress --no-audit"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "RC"
}
